# Ignore bundler config.
/.bundle

/config/initializers/ssl_verification.rb

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

.byebug_history

/config/local_env.yml

/config/master.key